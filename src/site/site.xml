<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://maven.apache.org/DECORATION/1.8.1"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.1 https://maven.apache.org/xsd/decoration-1.8.1.xsd">
  <body>
    <head><![CDATA[
<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
<link rel="manifest" href="/site.webmanifest">
<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
<meta name="apple-mobile-web-app-title" content="${project.name}">
<meta name="application-name" content="${project.name}">
<meta name="msapplication-TileColor" content="#da532c">
<meta name="theme-color" content="#ffffff">
    ]]></head>
    <links>
      <item href="https://www.jetbrains.com/help/idea/http-client-in-product-code-editor.html"
        name="HTTP Client"/>
      <item href="https://github.com/JetBrains/http-request-in-editor-spec"
        name="HTTP Request in Editor Specification"/>
    </links>
    <menu inherit="top" name="General Info">
      <item href="./changelog.html" name="Change Log"/>
      <item href="./contributing.html" name="Contributing"/>
    </menu>
    <menu inherit="bottom" name="Documentation">
      <item href="./maven-plugin.html" name="Maven plugin"/>
      <item href="./junit-extension.html" name="jUnit Extension"/>
      <item href="./autoconfiguration.html" name="Spring Boot Test autoconfiguration"/>
      <item href="./apidocs/" name="Javadocs"/>
    </menu>
    <menu inherit="bottom" name="Info and reports">
      <item href="./project-info.html" name="Project Information"/>
    </menu>
    <menu inherit="bottom" name="Code">
      <item href="${project.scm.url}" name="SCM">
        <description>GitLab</description>
      </item>
      <item href="${project.ciManagement.url}" name="CI">
        <description>${project.ciManagement.system}</description>
      </item>
      <item href="${project.issueManagement.url}" name="Issues">
        <description>${project.issueManagement.system}</description>
      </item>
    </menu>
    <menu inherit="bottom" name="Releases">
      <item href="${gitlab.release.url}" name="GitLab"/>
      <item href="${github.release.url}" name="GitHub"/>
      <item href="./changelog.html" name="Changes Log"/>
    </menu>
    <menu inherit="bottom" name="Icons">
      <item href="${project.scm.url}" img="gitlab" name="GitLab"/>
    </menu>
  </body>
  <custom>
    <skinConfig>
      <bootswatchStyle>flatly</bootswatchStyle>
      <bottomNav>
        <menu>General Info</menu>
        <menu>Code</menu>
        <menu>Releases</menu>
      </bottomNav>
      <descriptionUrl>${project.description}</descriptionUrl>
      <iconNav>
        <menu>Icons</menu>
      </iconNav>
      <keywords>ijhttp,http client cli,maven plugin,junit extension,spring boot test</keywords>
      <topNav>
        <menu>Documentation</menu>
        <menu>Info and reports</menu>
      </topNav>
    </skinConfig>
  </custom>
  <skin>
    <artifactId>docs-maven-skin</artifactId>
    <groupId>com.bernardomg.maven.skins</groupId>
    <version>${docs-maven-skin.version}</version>
  </skin>
</project>
